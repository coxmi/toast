{
  "name": "toast-static",
  "version": "0.1.9",
  "license": "CC0 1.0 Universal",
  "author": "Michael Cox <michael@se-make.co.uk>",
  "description": "tiny static site generator plugin for webpack",
  "homepage": "https://github.com/coxmi/toast",
  "bugs": "https://github.com/coxmi/toast/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coxmi/toast.git"
  },
  "keywords": [
    "static site generator",
    "webpack",
    "ssg"
  ],
  "files": [
    "README.md",
    "dist/*.js",
    "dist/*.js.map"
  ],
  "main": "dist/webpack.js",
  "scripts": {
    "build": "tsc; babel src --out-dir dist --extensions '.ts,.js,.jsx' --source-maps",
    "watch": "tsc --watch & babel src --out-dir dist --extensions '.ts,.js,.jsx' --source-maps --watch",
    "clean": "rm -rf dist",
    "test": "ava",
    "test-watch": "webpack --watch --config ./test/helpers/manual-watch-config.js",
    "snapshots": "ava --update-snapshots"
  },
  "ava": {
    "babel": {
      "extensions": [
        "ts",
        "js"
      ],
      "compileAsTests": [
        "test/helpers/**/*"
      ]
    }
  },
  "devDependencies": {
    "@ava/babel": "^2.0.0",
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.0",
    "@babel/plugin-transform-react-jsx": "^7.17.12",
    "@babel/preset-env": "^7.18.0",
    "@babel/preset-typescript": "^7.17.12",
    "ava": "^3.15.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-add-module-exports": "^1.0.4",
    "node-fetch": "3.2",
    "preact": "^10.7.2",
    "preact-render-to-string": "^5.2.0",
    "typescript": "^4.6.4",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "async-hooks-context": "^4.0.0",
    "clear-module": "^4.1.2",
    "fast-glob": "^3.2.11",
    "find-cache-dir": "^3.3.2",
    "fs-extra": "^10.1.0",
    "source-map-support": "^0.5.21"
  },
  "peerDependencies": {
    "webpack": "^5.58.2"
  }
}
